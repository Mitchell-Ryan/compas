{{ let result = ""; }}
{{ if (type === "boolean") { }}
    {{ result += "boolean"; }}
{{ } else if (type === "number") { }}
    {{ result += "number"; }}
{{ } else if (type === "string") { }}
    {{ result += "string"; }}
{{ } else if (type === "object") { }}
    {{ result += "{"; }}
    {{ result += Object.entries(it.keys).map(([k,v]) => k + ": " + validatorsJsDoc(v).trim()).join(", "); }}
    {{ result += "}"; }}
{{ } else if (type === "array") { }}
    {{ result += validatorsJsDoc(it.values).trim() + "[]"; }}
{{ } else if (type === "anyOf") { }}
    {{ result += "("; }}
    {{ result += it.values.map(item => validatorsJsDoc(item).trim()).join("|"); }}
    {{ result += ")"; }}
{{ } else if (type === "reference") { }}
    {{ result += it.referenceModel; }}
{{ } else if (type === "any") { }}
    {{ result += "*"; }}
{{ } else if (type === "generic") { }}
    {{ result += "Object.<"; }}
    {{ result += validatorsJsDoc(it.keys); }}
    {{ result += ","; }}
    {{ result += validatorsJsDoc(it.values); }}
    {{ result += ">"; }}
{{ } }}

{{ if (it.optional && it.default === undefined) { }}
    {{ result += "|undefined"; }}
{{ } }}
{{= result }}
